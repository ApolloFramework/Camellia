[X,Y] = meshgrid(linspace(0,1,100));

n = 1;
epsilon = .1;

lambda = n^2*pi^2*epsilon;
r1 = (1+sqrt(1+4*epsilon*lambda))/(2*epsilon);
r2 = (1-sqrt(1+4*epsilon*lambda))/(2*epsilon);

YY = zeros(size(Y));
for n = 1:25
    C = (cos(n*pi/3)-cos(2*n*pi/3))/(n*pi);
    YY = YY+C*sin(n*pi*Y);
end
% YY = ones(size(Y)).*double(Y<2/3 & Y>1/3);
XX = (r2*exp(r2)*exp(r1*X)-r1*exp(r1)*exp(r2*X))/(exp(r2)*r2-exp(r1)*r1);
U = XX.*YY;
surf(X,Y,U)